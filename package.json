  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "Qs": true,
      "CryptoJS": true,
      "Peer": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "no-alert": 0,
      "no-console": 0,
      "prefer-template": 0
    }
  }
}
